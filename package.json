    "history": "~1.17.0",
    "jsplumb": "1.7.9",
    "lodash.intersection": "~3.2.0",
    "lodash.merge": "~2.4.1",
    "lodash.pick": "~3.1.0",
    "lodash.uniq": "~3.2.2",
    "markdownz": "~4.0.2",
    "modal-form": "~2.3.0",
    "moment": "~2.9.0",
    "panoptes-client": "~2.0.0",
    "papaparse": "mholt/PapaParse#cada171",
    "react-component-resizable": "~0.3.0",
    "react": "~0.14.1",
    "react-dom": "^0.14.1",
    "react-router": "~1.0.0",
    "react-interpolate-component": "~0.8.0",
    "react-pullout": "~1.0.1",
    "react-select": "~0.9.1",
    "react-swipe": "~3.0.0",
    "react-translate-component": "~0.10.0",
    "sugar-client": "~1.0.1",
    "swipe-js-iso": "~2.0.1",
